{
  "name": "portfolio",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "precommit": "lint-staged",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "commit": "git cz"
  },
  "dependencies": {
    "@emotion/core": "^10.0.22",
    "gatsby": "^2.18.3",
    "gatsby-background-image": "^0.9.7",
    "gatsby-image": "^2.2.33",
    "gatsby-plugin-emotion": "^4.1.15",
    "gatsby-plugin-google-analytics": "^2.1.28",
    "gatsby-plugin-preact": "^3.1.26",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-robots-txt": "^1.5.0",
    "gatsby-plugin-sharp": "^2.3.3",
    "gatsby-plugin-sitemap": "^2.2.22",
    "gatsby-plugin-typography": "^2.3.17",
    "gatsby-remark-copy-linked-files": "^2.1.31",
    "gatsby-remark-images": "^3.1.34",
    "gatsby-remark-katex": "^3.1.26",
    "gatsby-remark-prismjs": "^3.3.25",
    "gatsby-remark-reading-time": "^1.0.1",
    "gatsby-remark-responsive-iframe": "^2.2.28",
    "gatsby-remark-smartypants": "^2.1.17",
    "gatsby-source-filesystem": "^2.1.38",
    "gatsby-transformer-remark": "^2.6.59",
    "gatsby-transformer-sharp": "^2.3.5",
    "katex": "^0.11.1",
    "preact": "^10.3.2",
    "prismjs": "^1.17.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.8.0",
    "react-typography": "^0.16.19",
    "surge": "^0.21.3",
    "typography": "^0.16.19",
    "typography-theme-fairy-gates": "^0.16.19"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.0.3",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.7.1",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^3.1.0",
    "lint-staged": "^9.4.3",
    "prettier": "^1.19.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kpfromer/portfolio"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run precommit",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
